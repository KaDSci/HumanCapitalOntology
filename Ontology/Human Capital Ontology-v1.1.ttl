###  http://KadSciOntologies/HCO-Ontology/AbsenceFomDuty
ns1:AbsenceFomDuty rdf:type owl:Class ;
                   rdfs:subClassOf cco:ont00000228 ;
                   rdfs:comment "The placement of this class and all of its subclasses warrants discussion and review. It is not clear that the available documentation used for definition sources can settle this matter. Some discussions of absence from duty and leave suggest that it is a temporal interval, but not all."@en ;
                   rdfs:label "Absence from Duty"@en ;
                   skos:definition "A Planned Act in which some Employee does not report to their job as usual."@en ;
                   cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/AbsenceWithoutLeave
ns1:AbsenceWithoutLeave rdf:type owl:Class ;
                        rdfs:subClassOf ns1:NonpayStatus ;
                        rdfs:comment "OPM definition: \"a nonpay status that covers an Absence from Duty which has not been approved.\""@en ;
                        rdfs:label "Absence Without Leave"@en ;
                        skos:definition "A Nonpay Status into which the Employee is placed because that Employee is an agent in an unapproved Absence from Duty."@en ;
                        cco:ont00001754 "https://www.opm.gov/policy-data-oversight/data-analysis-documentation/personnel-documentation/processing-personnel-actions/gppa35.pdf"@en ;
                        cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/AbsentUniformedService
ns1:AbsentUniformedService rdf:type owl:Class ;
                           rdfs:subClassOf ns1:LeaveOfAbsence ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty cco:ont00001819 ;
                                             owl:someValuesFrom ns1:PlacementOnAbsentUniformedServices
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty cco:ont00001833 ;
                                             owl:someValuesFrom [ owl:intersectionOf ( cco:ont00001017
                                                                                       [ rdf:type owl:Class ;
                                                                                         owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty obo:BFO_0000056 ;
                                                                                                         owl:someValuesFrom ns1:NonpayStatus
                                                                                                       ]
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty obo:BFO_0000056 ;
                                                                                                         owl:someValuesFrom ns1:PayStatus
                                                                                                       ]
                                                                                                     )
                                                                                       ]
                                                                                     ) ;
                                                                  rdf:type owl:Class
                                                                ]
                                           ] ;
                           rdfs:label "Absent - Uniformed Service"@en ;
                           skos:definition "A Leave of Absence in which the Employee is absent, whether in Pay Status or Nonpay Status, to perform duty with the uniformed services and the Employee has reemployment rights under the Uniformed Services Employment and Reemployment Rights Act (USERRA – 38 U.S.C. chapter 43)."@en ;
                           skos:scopeNote "This type of Leave of Absence includes both instances of Leave Without Pay and Paid Leave."@en ;
                           cco:ont00001754 "https://www.opm.gov/policy-data-oversight/data-analysis-documentation/personnel-documentation/processing-personnel-actions/gppa15.pdf"@en ,
                                           "https://www.opm.gov/policy-data-oversight/data-analysis-documentation/personnel-documentation/processing-personnel-actions/gppa35.pdf"@en ;
                           cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/AccountingAndBudgetOccupationalSeries
ns1:AccountingAndBudgetOccupationalSeries rdf:type owl:Class ;
                                          rdfs:subClassOf ns1:WhiteCollarOccupationalSeries ;
                                          rdfs:label "Accounting and Budget Occupational Series"@en ;
                                          skos:definition "A White Collar Occupational Series that is a subdivision of the Accounting and Budget Group."@en ;
                                          cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfChangingServiceComputationDate
ns1:ActOfChangingServiceComputationDate rdf:type owl:Class ;
                                        rdfs:subClassOf ns1:PersonnelAction ;
                                        rdfs:comment "Various different cases of changes to Employee's Service Computation Date are explained in Chapter 6, Subchapter 3 of the OPM Guide to Processing Personnel Actions. https://www.opm.gov/policy-data-oversight/data-analysis-documentation/personnel-documentation/servicecreditleave.pdf"@en ;
                                        rdfs:label "Act of Changing Service Computation Date"@en ;
                                        skos:definition "A Personnel Action in which an Employee's Service Computation Date is adjusted."@en ;
                                        cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfComplianceTrainingInstruction
ns1:ActOfComplianceTrainingInstruction rdf:type owl:Class ;
                                       owl:equivalentClass [ owl:intersectionOf ( ns1:ActOfEmployeeTrainingInstruction
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty cco:ont00001807 ;
                                                                                    owl:someValuesFrom cco:ont00001324
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ] ;
                                       rdfs:subClassOf ns1:ActOfEmployeeTrainingInstruction ;
                                       rdfs:label "Act of Compliance Training Instruction"@en ;
                                       skos:altLabel "Act of Mandatory Employee Training Instruction"@en ;
                                       skos:definition "An Act of Employee Training Instruction that a Company provide to its Employees and which is mandated by some law, agency, or policy external to the Company (such as state and federal regulations)."@en ;
                                       cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfEmployeePayment
ns1:ActOfEmployeePayment rdf:type owl:Class ;
                         rdfs:subClassOf cco:ont00000836 ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty obo:BFO_0000057 ;
                                           owl:someValuesFrom ns1:EmployeePay
                                         ] ;
                         rdfs:label "Act of Employee Payment"@en ;
                         skos:definition "An Act of Financial Instrument Use wherein some Employee Pay is transferred by an Organization to some Employee in exchange for the Employee performing Work Duties for that Organization."@en ;
                         cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfEmployeeSafetyTrainingInstruction
ns1:ActOfEmployeeSafetyTrainingInstruction rdf:type owl:Class ;
                                           rdfs:subClassOf ns1:ActOfEmployeeTrainingInstruction ;
                                           rdfs:comment "This is not a subclass of Compliance Training because not all instances of Safety Training are mandated externally. Safety Training includes training that a company may offer, but which is not required by law."@en ;
                                           rdfs:label "Act of Employee Safety Training Instruction"@en ;
                                           skos:definition "An Act of Employee Training Instruction wherein a company provides its Employees with knowledge of precautionary procedures intended to mitigate the risk of job related injuries or fatalities, to both its Employees and its customers, as well as improve its organizational health."@en ;
                                           cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfEmployeeSkillsTrainingInstruction
ns1:ActOfEmployeeSkillsTrainingInstruction rdf:type owl:Class ;
                                           rdfs:subClassOf ns1:ActOfEmployeeTrainingInstruction ;
                                           rdfs:comment "Employee skills training is a matter of developing new skills, but also helping show the employee how to apply their previously acquired skills to job specific tasks (the latter is particularly relevant in technical skills training). Note that many of the direct children of this class partially overlap in their instances."@en ;
                                           rdfs:label "Act of Employee Skills Training Instruction"@en ;
                                           skos:definition "An Act of Employee Training Instruction wherein some Agent imparts to some Employee the skills required to perform their job effectively, or teaches the Employee how to apply applicable, previously acquired skills to their job position."@en ;
                                           cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfEmployeeTrainingAcquisition
ns1:ActOfEmployeeTrainingAcquisition rdf:type owl:Class ;
                                     rdfs:subClassOf cco:ont00000687 ;
                                     rdfs:label "Act of Employee Training Acquisition"@en ;
                                     skos:definition "An Act of Training Acquisition performed by an Employee of a company by: i) acquiring knowledge of the company's policies, procedures and guidelines, as well as applicable laws and regulations; and/or ii) acquiring the skills required to effectively perform their role in the company, from some Agent or communication medium that realizes a capacity derived from its affiliation with the company."@en ;
                                     cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfEmployeeTrainingInstruction
ns1:ActOfEmployeeTrainingInstruction rdf:type owl:Class ;
                                     rdfs:subClassOf cco:ont00001075 ;
                                     rdfs:label "Act of Employee Training Instruction"@en ;
                                     skos:altLabel "Employee Training Program"@en ;
                                     skos:definition "An Act of Training Instruction performed by an Agent who realizes a capacity derived from the Agent's affiliation with some company by imparting to some Employee, either directly or indirectly through some communication medium: i) knowledge of the Employee's company's policies, procedures and guidelines, as well as applicable laws and regulations; and/or ii) the skills required to effectively perform their role in the company."@en ;
                                     cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfEnteringPhasedRetirementStatus
ns1:ActOfEnteringPhasedRetirementStatus rdf:type owl:Class ;
                                        rdfs:subClassOf ns1:PersonnelAction ;
                                        rdfs:label "Act of Entering Phased Retirement Status"@en ;
                                        skos:definition "A Personnel Action that places an Employee into Phased Retirement Status."@en ;
                                        cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfFederalAdministrativeWork
ns1:ActOfFederalAdministrativeWork rdf:type owl:Class ;
                                   rdfs:subClassOf cco:ont00000228 ;
                                   rdfs:label "Act of Federal Administrative Work"@en ;
                                   skos:altLabel "Administrative Work"@en ;
                                   skos:definition "A Planned Act in which some Agent analyzes, evaluates, modifies, or develops the basic programs, policies, and procedures which facilitate the day to day to operations of some Federal Agency."@en ;
                                   skos:prefLabel "Act of Federal Administrative Work"@en ;
                                   cco:ont00001754 "https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/positionclassificationintro.pdf"@en ;
                                   cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfFederalProgramManagement
ns1:ActOfFederalProgramManagement rdf:type owl:Class ;
                                  rdfs:subClassOf ns1:ActOfFederalAdministrativeWork ;
                                  rdfs:comment """Definition of Program from the OPM General Schedule Supervisory Guide: 

\"PROGRAM - The mission, functions, projects, activities, laws, rules, and regulations which an agency is authorized and funded by statute to administer and enforce. Exercise of delegated authority to carry out program functions and services constitutes the essential purpose for the establishment and continuing existence of an agency. The focus of a program may be on providing products and services to the public, State and local government, private industry, foreign countries, or Federal agencies. Most programs have an impact or effect which is external to the administering agency. In addition, comparable agencywide line or staff programs essential to the operation of an agency are considered programs in applying this guide; the impact of these programs may be limited to activities within one or a few Federal agencies.

A program may be professional, scientific, technical, administrative, or fiscal in nature. Typically, programs involve broad objectives such as: national defense; law enforcement; public health, safety, and well-being; collection of revenue; regulation of trade; collection and dissemination of information; and the delivery of benefits or services. However, specialized or staff programs may be considerably narrower in scope (e.g., merit systems protection; nuclear safety; and agencywide personnel or budget programs). Programs are usually of such magnitude that they must be carried out through a combination of line and staff functions.\""""@en ;
                                  rdfs:label "Act of Federal Program Management"@en ;
                                  skos:definition "An Act of Federal Administrative Work in which some Agent manages or directs, or assists in a line capacity in managing or directing, one or more Federal Agency Programs, including appropriate supporting service Organizations ."@en ;
                                  skos:prefLabel "Act of Federal Program Management"@en ;
                                  cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfFinanceManagement
ns1:ActOfFinanceManagement rdf:type owl:Class ;
                           rdfs:subClassOf ns1:ActOfFederalAdministrativeWork ;
                           rdfs:label "Act of Financial Management"@en ;
                           rdfs:seeAlso "http://KadSciOntologies/HCO-Ontology/FinancialManagementSeries"@en ;
                           skos:altLabel "FINANCE"@en ;
                           skos:definition "An Act of Federal Administrative work that involves directing, controlling, or coordinating the financial resources of an organizational segment, field establishment, bureau, department, independent agency, or other organizational entity of the Federal Government."@en ;
                           skos:prefLabel "Act of Financial Management"@en ;
                           cco:ont00001754 "https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/standards/0500/gs0505.pdf"@en ;
                           cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfFirst-LevelSupervision
ns1:ActOfFirst-LevelSupervision rdf:type owl:Class ;
                                rdfs:subClassOf ns1:ActOfSupervision ;
                                rdfs:label "Act of First-Level Supervision"@en ;
                                skos:definition "An Act of Supervision in which a Person bearing a Supervisor Role personally directs, without the use of other subordinate supervisors, the work of subordinate Persons who bear no Supervisor Role."@en ;
                                skos:prefLabel "Act of First-Level Supervision"@en ;
                                cco:ont00001754 "https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/functional-guides/gssg.pdf (p. 7)"@en ;
                                cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .


###  http://KadSciOntologies/HCO-Ontology/ActOfHumanResourceManagement
ns1:ActOfHumanResourceManagement rdf:type owl:Class ;
                                 rdfs:subClassOf ns1:ActOfFederalAdministrativeWork ;
                                 rdfs:label "Act of Human Resource Management"@en ;
                                 rdfs:seeAlso "http://KadSciOntologies/HCO-Ontology/HumanResourcesManagementSeries"@en ;
                                 skos:altLabel "Personnel"@en ;
