    rdfs:subClassOf <https://github.com/jonathanvajda/cco-d-acts/ont00000008> ;
    skos:definition "A Claimant Role that inheres in an Agent who meets the specific age and service requirements to be the recipient of an Immediate Annuity, which if realized, is realized in an Immediate Annuity."@en ;
    skos:prefLabel "Immediate Annuity Entitlement Role"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:IndefiniteOverseasLimitedAppointmentAction a owl:Class ;
