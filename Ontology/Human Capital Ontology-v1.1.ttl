    rdfs:subClassOf ns1:Annuity ;
    skos:definition "An Annuity that begins immediately upon separation from service after meeting the required age and service conditions."@en ;
    skos:prefLabel "Immediate Annuity"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:ImmediateAnnuityEntitlementRole a owl:Class ;
