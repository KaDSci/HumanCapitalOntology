    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PositionClassificationStandard a owl:Class ;
    rdfs:label "Position Classification Standard"@en ;
    rdfs:comment "\"Position classification standards provide information used in determining the occupational series and title for positions performing white collar work in the Federal Government. They also provide grading criteria for positions classified under the General Schedule (GS) Classification System.\" https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/#url=Standards"@en ;
    rdfs:subClassOf cco:ont00001298 ;
    skos:definition "A Document issued by the Office of Personnel Management that is designed to bear Information Content Entities that relate the the grade level definitions in Title 5, U.S. Code to specific work situations and thereby provide a basis for assigning each Position the appropriate title, series, and grade."@en ;
    cco:ont00001754 "https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/positionclassificationintro.pdf"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PositionDescription a owl:Class ;
    rdfs:label "Position Description"@en ;
    rdfs:comment """"The position description should define clearly the major duties assigned and the nature and extent of responsibility for carrying out those duties. Qualification requirements should be evident from reading the description, and specialized requirements not readily apparent from the description should be specifically mentioned and supported by the described duties."\r\r\r
https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/positionclassificationintro.pdf"""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cco:ont00001982 ;
            owl:someValuesFrom ns1:Position ],
        cco:ont00000853 ;
    skos:definition "A Descriptive Information Content Entity that describes the major duties, responsibilities, and supervisory relationships of a given Position."@en ;
    cco:ont00001754 "https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/positionclassificationintro.pdf"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PositionTitle a owl:Class ;
    rdfs:label "Position Title"@en ;
    rdfs:comment """"The purpose of a position title is to communicate an immediate understanding and identification of the job. Titles should be short, meaningful, and generally descriptive of the work performed. They should also be consistent with the occupational series titles established by OPM; for example, positions in occupational series involving analytical, clerical, examining, or investigating work should be titled analyst, clerk, examiner, or investigator. Once basic titles have been established for positions in a series, those titles should be used consistently throughout the agency." \r\r\r
https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/positionclassificationintro.pdf [p. 14]"""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cco:ont00001916 ;
            owl:someValuesFrom ns1:Position ],
        cco:ont00001014 ;
    skos:altLabel "Job Title"@en ;
    skos:definition "A Proper Name that is an official name for a particular Position."@en ;
    cco:ont00001754 "https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/positionclassificationintro.pdf [Under H, 'Titling Positions']"@en,
        "https://www.opm.gov/policy-data-oversight/data-analysis-documentation/personnel-documentation/processing-personnel-actions/gppa35.pdf"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PrimaryLawEnforcementOfficerPosition a owl:Class ;
    rdfs:label "Primary Law Enforcement Officer Position"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue ns1:5USC8336\(c\) ;
            owl:onProperty ns1:covered_by ],
        [ a owl:Restriction ;
            owl:hasValue ns1:5USC8412\(d\)\(e\) ;
            owl:onProperty ns1:covered_by ],
        ns1:LawEnforcementOfficerPosition ;
    skos:definition "A Law Enforcement Officer Position the primary duties of which are the investigation, apprehension, or detention of individuals suspected or convicted of offenses against the criminal laws of the United States."@en ;
    cco:ont00001754 """Definition of 'Primary Position' under 5 CFR 831.902\r\r\r
https://www.ecfr.gov/current/title-5/chapter-I/subchapter-B/part-831"""@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:ProhibitedPersonnelPractice a owl:Class ;
    rdfs:label "Prohibited Personnel Practice"@en ;
    rdfs:comment """The Merit System Principles are the framework and foundation for making all personnel decisions in the civil service.\r\r\r
https://www.opm.gov/policy-data-oversight/employee-relations/training/prohibited-personnel-practices.pdf"""@en,
        """There are four general categories of Prohibited Personnel Practices:\r\r\r
● Discrimination \r\r\r
● Hiring practices that offend the merit system \r\r\r
● Retaliation for protected activity (including whistleblowing) \r\r\r
● Catch-all: violation of law, rule or regulation that implement merit systems principles (including constitutional rights)\r\r\r
https://www.opm.gov/policy-data-oversight/employee-relations/training/prohibited-personnel-practices.pdf"""@en ;
    rdfs:subClassOf cco:ont00000228 ;
    skos:definition "A Planned Act described in 5 U.S. Code § 2302, subsection (b) that is in violation of the Merit System Principles."@en ;
    cco:ont00001754 "https://www.law.cornell.edu/uscode/text/5/2302"@en,
        "https://www.opm.gov/policy-data-oversight/employee-relations/training/prohibited-personnel-practices.pdf"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PublicSpeakingSkill a owl:Class ;
