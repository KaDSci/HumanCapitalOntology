    cco:ont00001754 "https://www.investopedia.com/terms/a/attrition.asp#"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:OutsourcedEmployeeTrainingAcquisition a owl:Class ;
    rdfs:label "Outsourced Employee Training Acquisition"@en ;
    rdfs:subClassOf ns1:ActOfEmployeeTrainingAcquisition ;
    skos:definition "An Act of Employee Training Acquisition wherein an Employee either learns certain skills needed to improve their job performance, or learns about unique and unfamiliar workplace procecures and strategies that may enhance the company's practices or streamline its operations, from an Agent from outside of the company."@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:OutsourcedEmployeeTrainingInstruction a owl:Class ;
    rdfs:label "Outsourced Employee Training Instruction"@en ;
    rdfs:comment """The last clause of the definition is included to capture the idea that external professionals often may serve to improve the company's organizational effectiveness by offering a unique persepctive on workplace procedures, introducing unfamiliar strategies that can enhance the company's own established practices or streamline its operations.\r\r\r
https://www.indeed.com/career-advice/career-development/example-of-training-program"""@en ;
    rdfs:subClassOf ns1:ActOfEmployeeTrainingInstruction ;
    skos:definition "An Act of Employee Training Instruction wherein a company hires an Agent from outside of that company to impart to its Employees certain skills needed to improve Employee job performance or introduce to its Employees unique and unfamiliar workplace procecures and strategies that may enhance the company's practices or streamline its operations."@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PersonnelActionReportingRequirement a owl:Class ;
    rdfs:label "Personnel Action Reporting Requirement"@en ;
    rdfs:comment """"The Office of Personnel Management's authority to prescribe reporting requirements covering personnel actions is in section 2951, title 5, United States Code." p. 2 of guide.\r\r\r
\r\r\r
See also: https://www.law.cornell.edu/uscode/text/5/2951#:~:text=5%20U.S.%20Code%20%C2%A7%202951%20%2D%20Reports%20to%20the%20Office%20of%20Personnel%20Management,-U.S.%20Code&text=the%20Office%20keep%20records%20of,L."""@en ;
    rdfs:subClassOf cco:ont00001324 ;
    skos:definition "An Action Regulation that requires some Agent to to report any Personnel Actions that they intend to take to the Office of Personal Management."@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PhasedRetirementAnnuity a owl:Class ;
    rdfs:label "Phased Retirement Annuity"@en ;
    rdfs:subClassOf ns1:Annuity ;
    skos:definition "An Annuity that is payable to an Employee under 5 U.S.C. 8412a before the Employee reaches Full Retirement Status."@en ;
    cco:ont00001754 "5 CFR 848.102 https://www.ecfr.gov/current/title-5/section-848.102"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

ns1:PhasedRetirementStatus a owl:Class ;
    rdfs:label "Phased Retirement Status"@en ;
    rdfs:comment "This role, if realized, is realized in a Phased Retirement Annuity."@en ;
    rdfs:subClassOf <https://github.com/jonathanvajda/cco-d-acts/ont00000008> ;
    skos:definition "A Claimant Role that inheres in an Employee who is concurrently employed in Phased Employment and is eligible to recieve a Phased Retirement Annuity."@en ;
    cco:ont00001754 "https://www.ecfr.gov/current/title-5/section-848.102"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI .

<http://KadSciOntologies/HCO-Ontology/PositionChange5CFR§210.102> a owl:Class ;
