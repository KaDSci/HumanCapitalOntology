ns1:CompetitiveServiceTermAppointmentAction rdf:type owl:Class ;
                                            owl:equivalentClass [ owl:intersectionOf ( ns1:TermAppointmentAction
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty cco:ont00001803 ;
                                                                                         owl:someValuesFrom [ owl:intersectionOf ( cco:ont00001226
                                                                                                                                   [ rdf:type owl:Restriction ;
