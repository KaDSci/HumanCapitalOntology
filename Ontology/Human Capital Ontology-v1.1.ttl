        owl:NamedIndividual ;
    rdfs:label "Patent Classifying Series"@en ;
    ns1:StandardOccupationalClassificationCodeBasedonOccupationalSeries """23-2099 - Legal Support Workers, All Other\r\r\r
\r\r\r
https://www.bls.gov/oes/current/oes232099.htm"""@en ;
    skos:definition "A Copyright, Patent, and Trademark Occupational Series that has as parts all professional, scientific, and technical Positions which are primarily concerned with developing and/or administering systems for the classification, for patent purposes, of the technological knowledge embodied in United States patents and related material."@en ;
    cco:ont00001754 "https://www.opm.gov/policy-data-oversight/classification-qualifications/classifying-general-schedule-positions/occupationalhandbook.pdf (p. 92)"@en ;
    cco:ont00001760 "http://KadSciOntologies/HCO-Ontology"^^xsd:anyURI ;
